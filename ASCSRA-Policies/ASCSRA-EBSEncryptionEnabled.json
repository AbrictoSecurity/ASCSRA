{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ec2:DetachVolume",
                "ec2:AttachVolume",
                "ec2:DeleteVolume",
                "ec2:CopySnapshot",
                "ec2:DeleteSnapshot",
                "ec2:StartInstances",
                "ec2:DescribeVolumes",
                "ec2:CreateSnapshot",
                "ec2:StopInstances",
                "ec2:CreateVolume"
            ],
            "Resource": "*"
        }
    ]
}
